# Example Terraform variables file
# Copy this to terraform.tfvars and update with your actual values

# Cloudflare API token (required for DNS management)
cf_api_token = "your-cloudflare-api-token-here"

# Domain name for your application
domain_name = "bareflux.co"

# GitHub repository containing your Node.js application
github_repo = "https://github.com/sumansaurabh/task-bb.git"

# Email address for SSL certificate registration
admin_email = "admin@bareflux.co"
